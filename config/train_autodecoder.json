{
  "name": "AutoEncoder",
  "n_gpu": 1,
  "amp": true,
  "checkpoint": null,
  "arch": {
    "type": "AutoEncoder",
    "args": {
      "num_in_feats": 25600,
      "num_latent_vector": 512,
      "num_enc_blocks": 4,
      "num_dec_blocks": 4,
      "dropout": 0.05,
      "add_IC": true
    }
  },
  "data_loader": {
    "type": "FeaturesDataLoader",
    "args": {
      "data_dir": "data",
      "batch_size": 32,
      "shuffle": true,
      "validation_split": 0.1,
      "num_workers": 8,
      "dataset": "feat_256_train",
      "dataset_test": "feat_256_test",
      "num_classes": 0
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 0.0026525028717740277,
      "weight_decay": 6.5025649191944946e-06
    }
  },
  "loss": {
    "type": "mse",
    "args": {}
  },
  "train_metrics": [
    "accuracy_mse"
  ],
  "test_metrics": [
    "accuracy_mse"
  ],
  "lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
      "gamma": 0.2921212208238253
    }
  },
  "trainer": {
    "epochs": 5,
    "save_dir": "saved/",
    "save_period": 1,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 5,
    "tensorboard": true
  }
}
